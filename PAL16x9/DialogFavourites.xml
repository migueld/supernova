<window type="dialog" id="105">
	<defaultcontrol always="true">450</defaultcontrol>
	<animation effect="fade" time="400">WindowOpen</animation>
	<animation effect="fade" time="400">WindowClose</animation>

	<coordinates>
		<system>1</system>
		<posx>25</posx>
		<posy>20</posy>
	</coordinates>

	<controls>

		<control type="image">
			<description>background image</description>
			<posx>-15</posx>
			<posy>-5</posy>
			<width>380</width>
			<height>360</height>
			<texture border="20">button-menu-background.png</texture>
		</control>
		<control type="group">
			<control type="list" id="450">
			<posx>-1</posx>
			<posy>15</posy>
			<width>348</width>
			<height>320</height>
				<onleft>1</onleft>
				<onright>10</onright>
				<onup>450</onup>
				<ondown>450</ondown>
				<scrolltime>200</scrolltime>
				<itemlayout height="32">
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>328</width>
						<height>32</height>
						<font>font12</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>button-text</textcolor>
						<selectedcolor>listselectedtext</selectedcolor>
						<info>ListItem.Label</info>
					</control>
				</itemlayout>
				<focusedlayout height="32">
					<control type="image">
						<width>348</width>
						<height>32</height>
						<posx>0</posx>
						<posy>0</posy>
						<texture>default-button.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>328</width>
						<height>32</height>
						<font>font12</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>button-text</textcolor>
						<selectedcolor>listselectedtext</selectedcolor>
						<info>ListItem.Label</info>
						<scroll>true</scroll>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>